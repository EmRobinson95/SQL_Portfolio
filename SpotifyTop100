CREATE TABLE top_spotify (
id VARCHAR(21) PRIMARY KEY,
name TEXT,
artists TEXT,
danceability DECIMAL,
energy DECIMAL,
key DECIMAL,
loudness DECIMAL,
mode DECIMAL,
speechiness DECIMAL,
acousticness DECIMAL,
instrumentalness DECIMAL,
liveness DECIMAL,
valence DECIMAL,
tempo DECIMAL,
duration_ms DECIMAL,
time_signature DECIMAL
 );

INSERT INTO top_spotify (id,name,artists,danceability,energy,key,loudness,
