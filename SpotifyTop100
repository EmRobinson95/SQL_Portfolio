/* Top songs based on danceability */
SELECT name, artists FROM top_spotify
    ORDER BY danceability DESC; 

/*Songs with a higher danceability and energy */
SELECT name, artists FROM top_spotify
    WHERE danceability > .6 AND energy > .7;

/*List sorted from slower to higher tempo*/
SELECT name, artists FROM top_spotify
    ORDER BY tempo ASC;

/*List of songs that are longer but in ascending order of tempo*/
SELECT name, artists FROM top_spotify
    WHERE duration_ms > 100000.0 
    ORDER BY tempo ASC;

